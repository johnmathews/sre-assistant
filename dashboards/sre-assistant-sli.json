{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "SLO Overview",
      "type": "stat",
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 0.99 }
            ]
          },
          "unit": "percentunit",
          "decimals": 3
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "background"
      },
      "targets": [
        {
          "expr": "sum(rate(sre_assistant_requests_total{status=\"success\"}[1h])) / sum(rate(sre_assistant_requests_total[1h]))",
          "legendFormat": "Availability (target >99.5%)",
          "refId": "A"
        },
        {
          "expr": "sum(rate(sre_assistant_tool_calls_total{status=\"success\"}[1h])) / sum(rate(sre_assistant_tool_calls_total[1h]))",
          "legendFormat": "Tool Success (target >99%)",
          "refId": "B"
        },
        {
          "expr": "1 - (sum(rate(sre_assistant_llm_calls_total{status=\"error\"}[1h])) / sum(rate(sre_assistant_llm_calls_total[1h])))",
          "legendFormat": "LLM Success (target >99%)",
          "refId": "C"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 1
    },
    {
      "title": "Request Latency p95 (target <15s)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 4 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 15 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(sre_assistant_request_duration_seconds_bucket{endpoint=\"/ask\"}[1h])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 2
    },
    {
      "title": "Request Latency Percentiles",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 18, "x": 6, "y": 4 },
      "fieldConfig": {
        "defaults": { "unit": "s", "custom": { "lineWidth": 2 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(sre_assistant_request_duration_seconds_bucket{endpoint=\"/ask\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(sre_assistant_request_duration_seconds_bucket{endpoint=\"/ask\"}[5m])) by (le))",
          "legendFormat": "p90",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(sre_assistant_request_duration_seconds_bucket{endpoint=\"/ask\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "C"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(sre_assistant_request_duration_seconds_bucket{endpoint=\"/ask\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "D"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 3
    },
    {
      "title": "Requests per Minute",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 12 },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "lineWidth": 2, "fillOpacity": 20 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(sre_assistant_requests_total{endpoint=\"/ask\", status=\"success\"}[5m])) * 60",
          "legendFormat": "success",
          "refId": "A"
        },
        {
          "expr": "sum(rate(sre_assistant_requests_total{endpoint=\"/ask\", status=\"error\"}[5m])) * 60",
          "legendFormat": "error",
          "refId": "B"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 4
    },
    {
      "title": "Tool Call Rate by Tool",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 9, "x": 6, "y": 12 },
      "fieldConfig": {
        "defaults": { "unit": "ops", "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (tool_name) (rate(sre_assistant_tool_calls_total{status=\"success\"}[5m]))",
          "legendFormat": "{{tool_name}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 5
    },
    {
      "title": "Tool Errors by Tool",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 9, "x": 15, "y": 12 },
      "fieldConfig": {
        "defaults": { "unit": "ops", "custom": { "lineWidth": 2, "fillOpacity": 10, "drawStyle": "bars" } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum by (tool_name) (rate(sre_assistant_tool_calls_total{status=\"error\"}[5m]))",
          "legendFormat": "{{tool_name}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 6
    },
    {
      "title": "Tool Call Duration p95",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "fieldConfig": {
        "defaults": { "unit": "s", "custom": { "lineWidth": 2 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (tool_name, le) (rate(sre_assistant_tool_call_duration_seconds_bucket[5m])))",
          "legendFormat": "{{tool_name}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 7
    },
    {
      "title": "LLM Token Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "fieldConfig": {
        "defaults": { "unit": "short", "custom": { "lineWidth": 2, "fillOpacity": 20 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(sre_assistant_llm_token_usage_total{type=\"prompt\"}[5m])) * 60",
          "legendFormat": "prompt tokens/min",
          "refId": "A"
        },
        {
          "expr": "sum(rate(sre_assistant_llm_token_usage_total{type=\"completion\"}[5m])) * 60",
          "legendFormat": "completion tokens/min",
          "refId": "B"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 8
    },
    {
      "title": "Estimated Cost (cumulative)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 28 },
      "fieldConfig": {
        "defaults": { "unit": "currencyUSD", "decimals": 4 },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "value"
      },
      "targets": [
        {
          "expr": "sre_assistant_llm_estimated_cost_dollars_total",
          "legendFormat": "Total Cost",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 9
    },
    {
      "title": "Cost Rate ($/hour)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 18, "x": 6, "y": 28 },
      "fieldConfig": {
        "defaults": { "unit": "currencyUSD", "custom": { "lineWidth": 2 } },
        "overrides": []
      },
      "targets": [
        {
          "expr": "rate(sre_assistant_llm_estimated_cost_dollars_total[5m]) * 3600",
          "legendFormat": "$/hour",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 10
    },
    {
      "title": "Component Health",
      "type": "stat",
      "gridPos": { "h": 4, "w": 24, "x": 0, "y": 36 },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } }, "type": "value" }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "colorMode": "background",
        "orientation": "horizontal"
      },
      "targets": [
        {
          "expr": "sre_assistant_component_healthy",
          "legendFormat": "{{component}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "id": 11
    }
  ],
  "schemaVersion": 39,
  "tags": ["sre-assistant", "sli", "slo"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "name": "DS_PROMETHEUS",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "SRE Assistant SLI/SLO",
  "uid": "sre-assistant-sli",
  "version": 1
}
